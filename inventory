[all]
pgsql-1 ansible_python_interpreter=/usr/bin/python3
pgsql-2 ansible_python_interpreter=/usr/bin/python3

[postgres]
pgsql-1 ansible_python_interpreter=/usr/bin/python3
pgsql-2 ansible_python_interpreter=/usr/bin/python3

[database]
pgsql-1 ansible_python_interpreter=/usr/bin/python3 pgsqlrep_role=master

[database_replica]
pgsql-2 ansible_python_interpreter=/usr/bin/python3 pgsqlrep_role=replica
